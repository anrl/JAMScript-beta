{// A sample config file for JView generation
  "path" : "./", // Defines The Path
  "pages" : [ // An Array of Pages to be Generated
    {
      "name" : "Data Panel", // Page name
      "route" : "/data-panel", // Page sub-route over the domain
      "layout" : "default", // Page Layout
      "panels" : [ // Each page is organized by multiple Panels, these panels can easily be moved around based on layout
        {
          "type": "FormSet", // FormSet API
          "items": [ // Define components to show on the FormSet, a FormSet API
            {
              "type": "select", // Item Type - dropdown select list
              "dispLabel": "device", // Show Name
              "value": "stores[0][0]", // Value mapping from store
              "list": [ // a list of dispLabel and items
                {"dispLabel": "Device1", // display label in the dropdown list
                  "id": "M" // id mapped to the dropdown list item
                }, {"dispLabel": "Device2",
                  "id": "H"
                }, {"dispLabel": "Device3",
                  "id": "L"
                }, {"dispLabel": "Device4",
                  "id": "Z"
                }
              ]
            },
            {
              "type": "text", // Item Type - text input
              "dispLabel": "X value", // display label Name
              "value": "stores[0][1]" // Value Mapping from Store
            },
            { "type": "text",
              "dispLabel": "Y value",
              "value": "map.y"
            },
            {
              "type": "select",
              "dispLabel": "Status",
              "value": "map.status",
              "list": [
                {
                  "dispLabel": "A",
                  "id": "A"
                },
                {
                  "dispLabel": "NA",
                  "id": "NA"
                }
              ]
            }
          ],
          "stores": [
            // An Array of observable stores that will be injected from JData
            [ "0", "0", "0", "0" ]
          ]
        }
      ]
    }
  ]
}